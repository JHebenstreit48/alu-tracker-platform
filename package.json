{
  "name": "alu-tracker-car-data",
  "version": "1.0.0",
  "description": "Database for all ALU car data",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/server.ts",
    "dev": "ts-node -r tsconfig-paths/register src/server.ts",
    "seed:cars": "ts-node -r tsconfig-paths/register src/scripts/DatabaseImports/importCars.ts",
    "seed:cars:selected": "ts-node -r tsconfig-paths/register src/scripts/DatabaseImports/importCarsSelected.ts",

    "seed:images": "ts-node -r tsconfig-paths/register src/scripts/DatabaseImports/importCarImages.ts",
    "seed:brands": "ts-node -r tsconfig-paths/register src/scripts/DatabaseImports/importBrands.ts",
    "seed:garage-levels": "ts-node -r tsconfig-paths/register src/scripts/DatabaseImports/importGarageLevelsFirebase.ts",
    "seed:legend-store": "ts-node -r tsconfig-paths/register src/scripts/DatabaseImports/importLegendStoreFirebase.ts",

    "seed:normalize": "ts-node -r tsconfig-paths/register src/scripts/DatabaseImports/Cars/seedsNormalize.ts",
    "seed:normalize:apply": "ts-node -r tsconfig-paths/register src/scripts/DatabaseImports/Cars/seedsNormalize.ts --apply",
    
    "seed:icons": "ts-node -r tsconfig-paths/register src/scripts/DatabaseImports/importIcons.ts",
    "seed:logos": "ts-node -r tsconfig-paths/register src/scripts/DatabaseImports/importLogos.ts",
    "ping:firebase": "ts-node -r tsconfig-paths/register src/scripts/pingFirestore.ts",
    "seed:diagnose": "ts-node -r tsconfig-paths/register src/scripts/Diagnosing/diagnoseCarSet.ts",
    "seed:backfill": "cross-env INCLUDE_PER_CAR_WITH_COLLECTOR=1 INCLUDE_CLASS_JSON_WITH_COLLECTOR=1 ts-node -r tsconfig-paths/register src/scripts/DatabaseImports/importCars.ts",
    "seed:count": "ts-node -r tsconfig-paths/register src/scripts/Diagnosing/seedsCount.ts",
    "db:count": "ts-node -r tsconfig-paths/register src/scripts/Diagnosing/dbCount.ts",
    "compare:seeds-db": "ts-node -r tsconfig-paths/register src/scripts/Diagnosing/compareSeedsToDb.ts",
    "clean:content": "ts-node -r tsconfig-paths/register src/scripts/maintenance/cleanContent.ts",
    "clean:content:allow": "cross-env ALLOW_CONTENT_DROP=1 ts-node -r tsconfig-paths/register src/scripts/maintenance/cleanContent.ts",
    "ts": "ts-node",
    "build": "tsc",
    "start:watch": "nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "start:dev": "npm run start:watch"
  },
  "author": "Justin Hebenstreit",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.18",
    "cross-env": "^10.1.0",
    "dotenv": "^16.4.5",
    "fast-glob": "^3.3.3",
    "inquirer": "^8.2.4",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "firebase-admin": "^13.6.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.4",
    "zod": "^4.1.12"
  }
}